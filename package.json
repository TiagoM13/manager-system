  "repository": "https://github.com/TiagoM13/manager-system.git",
  "author": "Tiago Mota",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended",
      "plugin:storybook/recommended",
      "plugin:storybook/recommended"
    ]
  },
  "resolutions": {
    "strip-ansi": "^6.0.1"
  }
