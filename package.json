    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.1",
    "react-loading-skeleton": "^3.4.0",
    "react-router-dom": "^6.22.3",
    "react-select": "^5.8.0",
    "react-toastify": "^10.0.5",
    "styled-components": "^6.1.8",
    "tailwind-merge": "^2.3.0",
    "ts-jest": "^29.1.2",
    "vite-plugin-environment": "^1.1.3",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
